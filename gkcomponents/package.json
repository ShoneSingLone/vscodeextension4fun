{
	"name": "vue-to-requiredjs",
	"displayName": "vue-to-requiredjs",
	"description": "",
	"publisher": "ShoneSingLone",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"activationEvents": [
		"onCommand:shone.sing.lone.getComponents"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "shone.sing.lone.getComponents",
				"title": "vue2js"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"configuration": {
		"title": "vueToRequiredjs",
		"properties": {
			"vueToRequiredjs.output.htmlPath": {
				"type": "string",
				"description": "path of out HTML"
			},
			"vueToRequiredjs.output.cssPath": {
				"type": "string",
				"description": "path of out CSS"
			},
			"vueToRequiredjs.output.jsPath": {
				"type": "string",
				"description": "path of out JS"
			}
		}
	},
	"categories": [
		"Other"
	],
	"galleryBanner": {
		"color": "#eff1f3",
		"theme": "light"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.4",
		"@babel/preset-env": "^7.3.1",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"autoprefixer": "^9.4.6",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^24.1.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-register": "^6.26.0",
		"browser-sync": "^2.26.3",
		"eslint": "^5.13.0",
		"eslint-plugin-html": "^5.0.0",
		"eslint-plugin-vue": "^5.1.0",
		"gulp": "^4.0.0",
		"gulp-babel": "^8.0.0",
		"gulp-postcss": "^8.0.0",
		"gulp-sass": "^4.0.2",
		"node-sass": "^4.11.0",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	}
}
